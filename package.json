{
  "workspaces": [
    "example"
  ],
  "version": "0.1.0",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test --passWithNoTests",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "peerDependencies": {
    "leaflet": ">=1.6.0",
    "react": ">=17.0",
    "react-dom": ">=17.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "name": "react-leaflet-fiber",
  "description": "Fiber renderer for Leaflet.js",
  "author": "Treasured Inc.",
  "module": "dist/react-leaflet-fiber.esm.js",
  "size-limit": [
    {
      "path": "dist/react-leaflet-fiber.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/react-leaflet-fiber.esm.js",
      "limit": "10 KB"
    }
  ],
  "dependencies": {
    "react-reconciler": "0.26.1"
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@size-limit/preset-small-lib": "^4.9.1",
    "@storybook/addon-essentials": "^6.1.11",
    "@storybook/addon-info": "^5.3.21",
    "@storybook/addon-links": "^6.1.11",
    "@storybook/addons": "^6.1.11",
    "@storybook/react": "^6.1.11",
    "@types/leaflet": "^1.5.19",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-reconciler": "^0.18.0",
    "babel-loader": "^8.2.2",
    "husky": "^4.3.6",
    "leaflet": "^1.7.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-is": "^17.0.1",
    "size-limit": "^4.9.1",
    "tsdx": "^0.14.1",
    "tslib": "^2.0.3",
    "typescript": "^4.1.3"
  }
}
